<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.1.0" client="site" method="upgrade">
    <name>Florida Pro Bono Matters</name>
    <author>SavvySuit</author>
    <version>1.0.0</version>
    <description>Places the FPBM catalog in an article.</description>
    <files>
        <filename>mod_fpbm.xml</filename>
        <filename module="mod_fpbm">mod_fpbm.php</filename>
        <filename>index.html</filename>
        <filename>helper.php</filename>
        <filename>tmpl/default.php</filename>
        <filename>tmpl/index.html</filename>
    </files>
    <config>
        <fields name="params">
            <fieldset name="basic">
                <field name="providerID" type="integer" default="-1" label="Your Provider ID" description="The ID provided to you by SavvySuit." first="-1" last="100" step="1" />
            </fieldset>
        </fields>
    </config>
</extension>
